ProcessConfig = pAcommsHandler_CGG
{
    modem_id: 2 
    common {
        log: false
        verbosity: QUIET show_gui: false
    }
    load_shared_library: "/home/rypkema/Workspace/moos-ivp-moossafir/lib/libmoossafir_protobuf.so"
    translator_entry {
        protobuf_name: "moossafir.Status"
        trigger { type: TRIGGER_TIME
            period: 5
        }
        create { technique: TECHNIQUE_FORMAT
            moos_var: "VEHICLE_ID"
            format: "%1%"
        }
        create { technique: TECHNIQUE_FORMAT
            moos_var: "NAV_X"
            format: "%10%"
        }
        create { technique: TECHNIQUE_FORMAT
            moos_var: "NAV_Y"
            format: "%11%"
        }
        create { technique: TECHNIQUE_FORMAT
            moos_var: "NAV_SPEED"
            format: "%12%"
        }
        create { technique: TECHNIQUE_FORMAT
            moos_var: "NAV_HEADING"
            format: "%13%"
            algorithm { name: "angle_0_360"
                primary_field: 13
            }
        }
        create { technique: TECHNIQUE_FORMAT
            moos_var: "NAV_DEPTH"
            format: "%14%"
        }
        create { technique: TECHNIQUE_FORMAT
            moos_var: "DRIFT_X"
            format: "%15%"
        }
        create { technique: TECHNIQUE_FORMAT
            moos_var: "DRIFT_Y"
            format: "%16%"
        }
        create { technique: TECHNIQUE_FORMAT
            moos_var: "CONSO_ALL"
            format: "%17%"
        }
        publish {
            technique: TECHNIQUE_PROTOBUF_TEXT_FORMAT
            moos_var: "COMMS_STATUS_IN"
        }
    }
    translator_entry {
        protobuf_name: "moossafir.MiniStatus"
        trigger { type: TRIGGER_TIME
            period: 1
        }
        create { technique: TECHNIQUE_FORMAT
            moos_var: "NAV_X_MOD30"
            format: "%10%"
        }
        create { technique: TECHNIQUE_FORMAT
            moos_var: "NAV_Y_MOD30"
            format: "%11%"
        }
        publish {
            technique: TECHNIQUE_PROTOBUF_TEXT_FORMAT
            moos_var: "COMMS_MINI_STATUS_IN"
        }
    }
    queue_cfg { 
        message_entry { protobuf_name: "moossafir.Status"  
            ack: false  
            blackout_time: 10  
            max_queue: 1
            ttl: 1800
            value_base: 1.5 
            role { type: SOURCE_ID  field: "node" }
        }
        message_entry { protobuf_name: "moossafir.MiniStatus"  
            ack: false
            blackout_time: 1
            max_queue: 1
            ttl: 1800
            value_base: 1.5 
        }
    }
    mac_cfg {
        type: MAC_FIXED_DECENTRALIZED
        slot { src: 2  type: DATA  slot_seconds: 30 rate: 2 } // 0
        slot { src: 2  type: DATA  slot_seconds: 30 rate: 0 } // 1
        slot { src: 2  type: DATA  slot_seconds: 30 rate: 0 } // 2
        slot { src: 2  type: DATA  slot_seconds: 30 rate: 0 } // 3
        slot { src: 2  type: DATA  slot_seconds: 30 rate: 0 } // 4
        slot { src: 2  type: DATA  slot_seconds: 30 rate: 1 } // 5
        slot { src: 2  type: DATA  slot_seconds: 30 rate: 0 } // 6
        slot { src: 2  type: DATA  slot_seconds: 30 rate: 0 } // 7
        slot { src: 2  type: DATA  slot_seconds: 30 rate: 0 } // 8
        slot { src: 2  type: DATA  slot_seconds: 30 rate: 0 } // 9
        slot { src: 2  type: DATA  slot_seconds: 30 rate: 1 } // 10
        slot { src: 2  type: DATA  slot_seconds: 30 rate: 0 } // 11
        slot { src: 2  type: DATA  slot_seconds: 30 rate: 0 } // 12
        slot { src: 2  type: DATA  slot_seconds: 30 rate: 0 } // 13 
        slot { src: 2  type: DATA  slot_seconds: 30 rate: 0 } // 14
    }
    dccl_cfg { id_codec: "moossafir.id" }
    driver_cfg 
